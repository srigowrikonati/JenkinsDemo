                            }
                        }
                        stage('two'){
                               steps{
                                     input ('Do you want to proceedSS?')
                                    }
                              }
                           stage('Three'){
                                   when {
                                          not {
                                                  branch "master"
                                                }
                                        }
                                              steps {
                                                      echno "Hello"
                                                }
                                          } 
                                          stage('four') {
                                                           parallel {
                                                                  stage('Unit test') {
                                                                                steps {
                                                                                 echo "running the unit test...."
                                                                             }
                                                                          }
stage ('Integreation test') {
agent {
docker {
reuseNode false
image 'ubantu'
}
}
steps {
echo 'running the integration test..'
}
}
}
}
}
